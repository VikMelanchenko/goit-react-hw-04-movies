Cast characters={characters} />;\n  }\n}\n","/Users/viktoriiamelanchenko/Documents/GitHub/goit-react-hw-04-movies/src/components/SearchBar/SearchBar.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/goit-react-hw-04-movies/src/components/FetchMovieRequest/FetchMovieRequest.jsx",["81"],"import { useState, useEffect } from 'react';\nimport { fetchSearchMovie } from '../../service/api_movie';\nimport Spinner from '../Loader/Loader';\nimport MovieCard from '../MovieCard/MovieCard';\nimport PropTypes from 'prop-types';\n\nconst Status = {\n  IDLE: 'idle',\n  PENDING: 'pending',\n  RESOLVED: 'resolved',\n  REJECTED: 'rejected',\n};\n\nexport default function FetchMovieRequest({ query }) {\n  const [movies, setMovies] = useState([]);\n  const [status, setStatus] = useState(Status.IDLE);\n  const [error, setError] = useState(null);\n\n  useEffect(() => {\n    if (!query) {\n      return;\n    }\n\n    setStatus(Status.PENDING);\n    fetchMovies();\n    setStatus(Status.RESOLVED);\n  }, [query]);\n\n  const fetchMovies = () => {\n    fetchSearchMovie(query)\n      .then(({ results }) => {\n        setMovies(results);\n      })\n      .catch((error) => {\n        setError(error);\n        setStatus(Status.REJECTED);\n      });\n  };\n\n  if (status === Status.IDLE) {\n    return (\n      <div style={{ textAlign: 'left' }}>\n        Please enter a search term to begin your search...\n      </div>\n    );\n  }\n\n  if (status === Status.PENDING) {\n    return <Spinner />;\n  }\n\n  if (status === Status.REJECTED) {\n    return <div>{error.message}</div>;\n  }\n\n  if (status === Status.RESOLVED) {\n    return <>{movies && <MovieCard movies={movies} />}</>;\n  }\n}\n\nFetchMovieRequest.propTypes = {\n  query: PropTypes.string,\n};\n","/Users/viktoriiamelanchenko/Documents/GitHub/goit-react-hw-04-movies/src/pages/ReviewsView/Reviews.jsx",["82"],"import { useState, useEffect } from 'react';\nimport { fetchMovieReviews } from '../../service/api_movie';\nimport ReviewsList from 