mponents/AppBar/AppBar';\nimport Container from './components/Container/Container';\n\n// Разделение кода /динамические страницы/\nconst HomeView = lazy(() =>\n  import(`./pages/HomeView/HomeView` /* webpackChunkName: \"home-view\" */)\n);\nconst MoviesView = lazy(() =>\n  import(`./pages/MoviesView/MoviesView` /* webpackChunkName: \"movies-view\" */)\n);\nconst NotFoundView = lazy(() =>\n  import(\n    `./pages/NotFoundView/NotFoundView` /* webpackChunkName: \"not-found-page-view\" */\n  )\n);\nconst MovieDetailsView = lazy(() =>\n  import(\n    `./pages/MovieDetailsView/MovieDetailsView` /* webpackChunkName: \"movie-details-view\" */\n  )\n);\n\nexport default function App() {\n  return (\n    <Container>\n      <AppBar />\n\n      <Suspense fallback={<div>Loading...</div>}>\n        <Switch>\n          <Route path=\"/\" exact>\n            <HomeView />\n          </Route>\n\n          <Route path=\"/movies\" exact>\n            <MoviesView />\n          </Route>\n\n          <Route path=\"/movies/:movieId\">\n            <MovieDetailsView />\n          </Route>\n\n          <Route>\n            <NotFoundView />\n          </Route>\n        </Switch>\n      </Suspense>\n    </Container>\n  );\n}\n","/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/pages/HomeView/HomeView.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/components/AppBar/AppBar.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/components/Container/Container.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/pages/NotFoundView/NotFoundView.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/pages/MoviesView/MoviesView.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/pages/MovieDetailsView/MovieDetailsView.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/service/api_movie.js",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/components/FetchMovieRequest/FetchMovieRequest.jsx",["103"],"import { useState, useEffect } from 'react';\nimport { fetchSearchMovie } from '../../service/api_movie';\nimport Spinner from '../Loader/Loader';\nimport MovieCard from '../MovieCard/MovieCard';\n\nconst Status = {\n  IDLE: 'idle',\n  PENDING: 'pending',\n  RESOLVED: 'resolved',\n  REJECTED: 'rejected',\n};\n\nexport default function FetchMovieRequest({ query }) {\n  const [movies, setMovies] = useState([]);\n  const [status, setStatus] = useState(Status.IDLE);\n  const [error, setError] = useState(null);\n\n  useEffect(() => {\n    if (!query) {\n      return;\n    }\n\n    setStatus(Status.PENDING);\n    fetchMovies();\n    setStatus(Status.RESOLVED);\n  }, [query]);\n\n  const fetchMovies = () => {\n    fetchSearchMovie(query)\n      .then(({ results }) => {\n        setMovies(results);\n        if (!results.length) {\n          throw new Error('Hmm...Nothing here. Try another search.');\n        }\n      })\n      .catch((error) => {\n        setError(error);\n        setStatus(Status.REJECTED);\n      });\n  };\n\n  if (status === Status.IDLE) {\n    return (\n      <div style={{ textAlign: 'left' }}>\n        Please enter a search term to begin your search...\n      </div>\n    );\n  }\n\n  if (status === Status.PENDING) {\n    return <Spinner />;\n  }\n\n  if (status === Status.REJECTED) {\n    return <div>{error.message}</div>;\n  }\n\n  if (status === Status.RESOLVED) {\n    return <>{movies && <MovieCard movies={movies} />}</>;\n  }\n}\n","/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/components/Navigation/Navigation.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/components/SearchBar/SearchBar.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/components/Loader/Loader.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/components/Reviews/Reviews.jsx",["104"],"import { useState, useEffect } from 'react';\nimport { fetchMovieReviews } from '../../service/api_movie';\nimport ReviewsList from '../ReviewsList/ReviewsList';\n\nimport Spinner from '../Loader/Loader';\n\nconst Status = {\n  IDLE: 'idle',\n  PENDING: 'pending',\n  RESOLVED: 'resolved',\n  REJECTED: 'rejected',\n};\n\nexport default function Reviews({ movieId }) {\n  const [reviews, setReviews] = useState([]);\n  const [status, setStatus] = useState(Status.IDLE);\n  const [error, setError] = useState(null);\n\n  useEffect(() => {\n    setStatus(Status.PENDING);\n    getReviews();\n    setStatus(Status.RESOLVED);\n  }, [movieId]);\n\n  const getReviews = () => {\n    fetchMovieReviews(movieId)\n      .then((request) => setReviews(request.results))\n      .catch((error) => {\n        setError(error);\n        setStatus(Status.REJECTED);\n      });\n  };\n\n  if (status === Status.IDLE) {\n    return <ReviewsList />;\n  }\n\n  if (status === Status.PENDING) {\n    return <Spinner />;\n  }\n\n  if (status === Status.REJECTED) {\n    return <div>{error.message}</div>;\n  }\n\n  if (status === Status.RESOLVED) {\n    return <ReviewsList reviews={reviews} />;\n  }\n}\n","/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/components/Cast/Cast.jsx",["105"],"import { useState, useEffect } from 'react';\nimport { fetchMovieCredits } from '../../service/api_movie';\nimport Cast from '../CastItem/CastItem';\nimport Spinner from '../Loader/Loader';\n\nconst Status = {\n  IDLE: 'idle',\n  PENDING: 'pending',\n  RESOLVED: 'resolved',\n  REJECTED: 'rejected',\n};\n\nexport default function CastView({ movieId }) {\n  const [characters, setCharacters] = useState([]);\n  const [status, setStatus] = useState(Status.IDLE);\n  const [error, setError] = useState(null);\n\n  useEffect(() => {\n    setStatus(Status.PENDING);\n    getCharacters();\n    setStatus(Status.RESOLVED);\n  }, [movieId]);\n\n  const getCharacters = () => {\n    fetchMovieCredits(movieId)\n      .then((request) => setCharacters(request.cast))\n      .catch((error) => {\n        setError(error);\n        setStatus(Status.REJECTED);\n      });\n  };\n\n  if (status === Status.IDLE) {\n    return <Cast />;\n  }\n\n  if (status === Status.PENDING) {\n    return <Spinner />;\n  }\n\n  if (status === Status.REJECTED) {\n    return <div>{error.message}</div>;\n  }\n\n  if (status === Status.RESOLVED) {\n    return <Cast characters={characters} />;\n  }\n}\n","/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/components/MovieCard/MovieCard.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/components/CastItem/CastItem.jsx",[],["106","107"],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/components/ReviewsList/ReviewsList.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/theme.js",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/global.js",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/components/ThemeSwither/ThemeSwither.jsx",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/components/Toggle/Toggle.js",[],"/Users/viktoriiamelanchenko/Documents/GitHub/GOIT/React/goit-react-hw-04-movies/src/useDarkMode.js",[],{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","severity":1,"message":"113","line":2,"column":17,"nodeType":"114","messageId":"115","endLine":2,"endColumn":25},{"ruleId":"116","severity":1,"message":"117","line":26,"column":6,"nodeType":"118","endLine":26,"endColumn":13,"suggestions":"119"},{"ruleId":"116","severity":1,"message":"120","line":23,"column":6,"node